    for(i =1; i < n; i++){
        key = a[i];
        j = i -1;
        while(j>=0 && key<a[j]){
            a[j+1] = a[j];
            --j;
        }
        a[j+1] = key;
        if(i == (n-1)/2){
            for(print = 0; print < n; print++){
                fprintf(stderr, "%d\n", a[print]);
            }
        }
    }
}


void insertion_sort(int *a, int n){
    int key, i, j, p;
    for(i = 0; i < n; i++){
        key = a[i];
        j = i -1;
        while(j>=0 && key<a[j]){
            a[j+1] = a[j];
            --j;
        }
        a[j+1] = key;
        if(i == (n-1)/2){
            for(p =0 ; p < n; p++){
                fprintf(stderr, "%d\n", a[p]);
            }
        }
    }
}


